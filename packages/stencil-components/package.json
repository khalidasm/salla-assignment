{
  "name": "@salla-assignment/stencil-components",
  "version": "0.0.1",
  "description": "Stencil Components with Tailwind",
  "main": "dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs && npm run poststencil",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "poststencil": "sed -i '' 's/import { OverlayEventDetail }/import type { OverlayEventDetail }/g' ../react-app/src/components/react-component-lib/createOverlayComponent.tsx && sed -i '' 's|<Overlay {...props} forwardedRef={ref} />|<Overlay {...(props as any)} forwardedRef={ref} />|g' ../react-app/src/components/react-component-lib/createOverlayComponent.tsx",
    "gh-pages": "stencil build --prod && gh-pages -d www"
  },
  "dependencies": {
    "@stencil/core": "^4.29.1",
    "@stencil/react-output-target": "^1.0.2"
  },
  "devDependencies": {
    "@stencil/sass": "^3.2.1",
    "@stencil/vue-output-target": "^0.10.7",
    "@tailwindcss/cli": "^4.1.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/jest": "^29.5.14",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^20.9.0",
    "stencil-tailwind-plugin": "^2.0.3",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3"
  },
  "license": "MIT"
}
